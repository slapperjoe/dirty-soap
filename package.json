{
  "name": "dirty-soap",
  "publisher": "dev1",
  "displayName": "Dirty SOAP",
  "description": "Visual SOAP client for VS Code",
  "repository": {
    "type": "git",
    "url": "https://github.com/slapperjoe/dirty-soap.git"
  },
  "icon": "images/logo.png",
  "license": "MIT",
  "version": "0.8.1",
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "dirty-soap.openInterface",
        "title": "Open Interface",
        "category": "Dirty Soap",
        "icon": "$(zap)"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "npm run compile-webview && tsc -p ./",
    "compile-webview": "cd webview && npm install && npm run build",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "dev:webview": "cd webview && npm run dev",
    "version-bump-major": "node scripts/bump.js major",
    "version-bump-minor": "node scripts/bump.js minor",
    "version-bump-patch": "node scripts/bump.js patch",
    "prepare": "npx simple-git-hooks"
  },
  "simple-git-hooks": {
    "pre-commit": ""
  },
  "devDependencies": {
    "@playwright/test": "^1.57.0",
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "20.2.5",
    "@types/sax": "^1.2.7",
    "@types/selfsigned": "^2.0.4",
    "@types/vscode": "^1.80.0",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "@vitest/coverage-v8": "^4.0.16",
    "@vscode/test-electron": "^2.3.2",
    "eslint": "^8.41.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "pkg": "^5.8.1",
    "simple-git-hooks": "^2.13.1",
    "typescript": "^5.1.3",
    "vitest": "^4.0.16"
  },
  "dependencies": {
    "@types/uuid": "^10.0.0",
    "axios": "^1.13.2",
    "fast-xml-parser": "^5.3.3",
    "https-proxy-agent": "^7.0.6",
    "jsonc-parser": "^3.3.1",
    "selfsigned": "^5.4.0",
    "soap": "^1.0.0",
    "uuid": "^13.0.0"
  }
}
